            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import groovy.json.JsonBuilder

def noticePolicyId = JMeterUtils.getProperty(&quot;noticePolicyId&quot;);
def noticePolicyName = JMeterUtils.getProperty(&quot;noticePolicyName&quot;);
def noticePolicyDescription = JMeterUtils.getProperty(&quot;noticePolicyDescription&quot;);

def numberOfTemplates = ${numberOfTemplates};
def numberOfCheckout = ${numberOfCheckout};

JMeterUtils.setProperty(&quot;numberOfTemplates&quot;, &quot;${numberOfTemplates}&quot;);
JMeterUtils.setProperty(&quot;numberOfCheckout&quot;, &quot;${numberOfCheckout}&quot;);

def templateId = JMeterUtils.getProperty(&quot;templateId&quot;);

def templateIds = new String[numberOfTemplates];
for(int i = 0; i &lt; templateIds.length; i++) {
    templateIds[i] = templateId;
}

log.info(&quot; &gt;&gt;&gt; templateIds: &quot; + templateIds);

