def response = prev.getResponseDataAsString();
log.info(&quot;Existing circulation rules: &quot; + response);

def rulesJsonWithLoan = updateRules(response, &quot; l &quot;, &quot;loanPolicyIdFromRules&quot;);
def rulesJsonWithNoticePolicy = updateRules(rulesJsonWithLoan, &quot; n &quot;, &quot;noticePolicyIdFromRules&quot;);

JMeterUtils.setProperty(&quot;rulesJson&quot;, rulesJsonWithNoticePolicy);
log.info(&quot;Updated circulation rules: &quot; + rulesJsonWithNoticePolicy);

def updateRules(def response, def charIndex, def propertyName) {
	def firstIndexOf = response.indexOf(charIndex);
	def lastIndexOff = response.lastIndexOf(charIndex);

	def firstPolicyId = findId(response, firstIndexOf);
	def lastPolicyId = findId(response, lastIndexOff);

	JMeterUtils.setProperty(propertyName, firstPolicyId);
	log.info(&quot;Circulation rules: &quot; + propertyName + &quot; = &quot; + firstPolicyId);
	
	return response.replace(lastPolicyId, firstPolicyId);
} 

def findId(def response, def indexOf) {
	def shiftRight = 2;
	def shiftLeft = 40;
	def exclusionChar = &quot;\&quot;&quot;;
	return response
			.substring(indexOf + shiftRight, indexOf + shiftLeft)
			.trim()
			.replace(exclusionChar,&quot;&quot;);
}</stringProp>
