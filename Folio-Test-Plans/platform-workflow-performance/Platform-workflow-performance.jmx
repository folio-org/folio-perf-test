          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import groovy.json.JsonSlurper;

// pick a random identifier from the sample data
def jsonSlurper = new JsonSlurper();
def resp = jsonSlurper.parseText(props.get(&quot;ids&quot;));
def size = resp.instances.size();
def fallback = true;
if (size &gt; 0) {
  // find a good candidate
  Random rand = new Random();
  def cnt = 0;
  while (fallback &amp;&amp; cnt &lt; 10) {
    def pick = rand.nextInt(size);
    def obj = resp.instances[pick].identifiers;
    for (i = 0; i &lt; obj.size(); i++) {
      // split to handle data like &quot;0889222177 (pbk.)&quot;
      def val = obj[i].value.trim().split()[0];
      // regex to skip data like &quot;(DE-599)ZDB2905315-8&quot;
      if (val ==~ /[0-9A-Za-z\-\_]+/) {
        vars.put(&quot;idType&quot;, obj[i].identifierTypeId);
        vars.put(&quot;idVal&quot;, val);
        vars.put(&quot;assertion&quot;, &quot;true&quot;);
        fallback = false;
        break;
      } else {
        log.info(&quot;skip sample identifier: &quot; + obj[i].value);
      }
    }
    cnt++;
  }
}
if (fallback) {
  log.warn(&quot;fall back to use default identifier type/value, no assertion&quot;);
  vars.remove(&quot;assertion&quot;);
  vars.put(&quot;idType&quot;, &quot;${DefaultIdType}&quot;);	
  vars.put(&quot;idVal&quot;, &quot;${DefaultIdValue}&quot;);	
}
log.info(&quot;picked identifier type: &quot; + vars.get(&quot;idType&quot;));
log.info(&quot;picked identifier value: &quot; + vars.get(&quot;idVal&quot;));</stringProp>
